# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
!/log/.keep
/tmp


.ruby-version
.ruby-gemset

config/database.yml
.idea/

# Ignore uploaded files
/public/system
/public/tmp
/public/uploads
/public/spec

.powrc

*.sublime-project
*.sublime-workspace

# Ignore test-related files
/coverage.data
/coverage/*
